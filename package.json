{
  "name": "parcel-react-typescript",
  "version": "1.0.0",
  "author": "Keypom",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "dev": "pnpm clean && pnpm static && parcel src/index.html -p 3000",
    "build": "pnpm static && parcel build src/index.html --public-url ./ --no-cache --no-source-maps",
    "static": "rm -rf dist && mkdir dist && cp -a ./public/* ./dist",
    "serve": "pnpm build && npx serve dist -s",
    "check": "tsc --noEmit",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.1",
    "@chakra-ui/icons": "2.0.14",
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/styled-system": "2.5.0",
    "@chakra-ui/system": "2.3.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@near-wallet-selector/core": "^7.6.1",
    "@near-wallet-selector/modal-ui": "^7.6.1",
    "@near-wallet-selector/near-wallet": "^7.6.1",
    "framer-motion": "^8.5.5",
    "keypom-js": "1.3.0-rc.1",
    "lodash": "^4.17.21",
    "near-api-js": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.8.0",
    "rxjs": "^7.8.0",
    "string_decoder": "^1.3.0",
    "swr": "^2.0.3",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@parcel/config-default": "^2.8.3",
    "@parcel/transformer-svg-react": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/validator-typescript": "^2.8.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.50.0",
    "buffer": "^5.5.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "events": "^3.3.0",
    "parcel": "^2.8.3",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "parcel-resolver-tspaths": "^0.0.9",
    "parcel-resolver-typescript-module": "^0.1.8",
    "prettier": "^2.8.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
